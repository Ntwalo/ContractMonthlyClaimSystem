<Window x:Class="ContractMonthlyClaimSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Contract Monthly Claim System" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Submit Claim" Click="SubmitClaim_Click" Margin="10"/>
            <Button Content="Verify Claim" Click="VerifyClaim_Click" Margin="10"/>
            <Button Content="Approve Claim" Click="ApproveClaim_Click" Margin="10"/>
            <Button Content="Upload Documents" Click="UploadDocuments_Click" Margin="10"/>
            <Button Content="Track Status" Click="TrackStatus_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
